package practise;

import java.util.Arrays;

public class maxNumArray {

	public static void main(String[] args) {
		
		
		int[] a = {3,1,2,5,6,4,3,7,6};
		
	 Arrays.sort(a);
	 int max = 0;
	 
	 for(int i = 0;i<a.length;i++)
	 {
		maximum(a[i]);
	 }
	 

	

	int maximum(int i) {
		
		if(i>max) {
			return i;
		}
		
		
		
		
		
	}

}

}
