package practise;

import java.util.Arrays;

public class maxNumArray {
	
	static int max = 0;
	static int arrayMaxVal;

	public static void main(String[] args) {
		
		int[] a = {3,1,2,5,6,4,3,7,6};
		Arrays.sort(a);
		for(int i = 0;i<a.length;i++)
		{
			arrayMaxVal =maximum(a[i]);
		}
		System.out.println("Max val from the given array is "+arrayMaxVal);
	 
	}
	
	private static int maximum(int i) {
		if(i>max) {
			return i;
		}
		return i;
	}

}
